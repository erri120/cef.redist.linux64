    <files>
        <file src="LICENSE.txt" target=""/>
        <file src="README.md" target=""/>

        <file src="cef.redist.linux64.props" target="build\"/>

        <file src="$cef_release_dir$/**" target="CEF\"/>
        <file src="$cef_resources_dir$/**" target="CEF\"/>
        <file src="$cef_license_file$" target="CEF\"/>
